#show allResults

{root} = eu.basysbio.cisd
{dss} = ${root}.dss
{openbis} = ch.systemsx.cisd.openbis

######################################################################
# Check dependencies to openbis

[dss] = ${dss}.*
[private_openbis] = ${openbis}.* excluding [dss] ${openbis}.dss.generic.shared.* ${openbis}.generic.shared.*

check sets [dss]

check [dss] independentOf [private_openbis]

